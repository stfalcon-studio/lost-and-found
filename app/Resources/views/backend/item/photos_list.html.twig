{% extends '@SonataAdmin/CRUD/base_show_field.html.twig' %}

{% block field %}
    {% for photo in object.photos %}
        {% if photo %}
            <img src="{{ vich_uploader_asset(photo, 'imageFile') }}" width="32" height="32"/>
        {% else %}
            none
        {% endif %}
    {% endfor %}
{% endblock %}
